<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierContactEntity, Ets.SingleApi.Model" table="SupplierContact" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="SupplierContactId" column="SupplierContactID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The foreign keys-->
		<many-to-one name="Contact" class="Ets.SingleApi.Model.Repository.ContactEntity, Ets.SingleApi.Model" column="ContactID" />
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierID" />
	</class>
</hibernate-mapping>