<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.PackageContentEntity, Ets.SingleApi.Model" table="PackageContent" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="PackageContentId" column="PackageContentID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="PackageClassificationId" column="PackageClassificationID" type="Int32" />
		<property name="SelectNumber" column="SelectNumber" type="Int32" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierID" />
		<many-to-one name="SupplierDish" class="Ets.SingleApi.Model.Repository.SupplierDishEntity, Ets.SingleApi.Model" column="SupplierDishID" />
	</class>
</hibernate-mapping>