<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.OrderSourceEntity, Ets.SingleApi.Model" table="OrderSource" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="OrderSourceId" column="OrderSourceId" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="OrderSourceName" column="OrderSourceName" type="String" not-null="false" />
	</class>
</hibernate-mapping>