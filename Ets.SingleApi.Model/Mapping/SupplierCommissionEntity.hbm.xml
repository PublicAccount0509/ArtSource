<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierCommissionEntity, Ets.SingleApi.Model" table="SupplierCommission" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="SupplierCommissionId" column="SupplierCommissionID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="DeliveryId" column="DeliveryID" type="Int32" />
		<property name="Total" column="Total" type="Decimal" />
		<property name="Commission" column="Commission" type="Decimal" />
		<property name="IsPaId" column="IsPaid" type="Boolean" />
		<property name="Canncelled" column="Canncelled" type="Boolean" not-null="false" />
		<property name="DateAdded" column="DateAdded" type="Timestamp" not-null="false" />
		<property name="DatePaId" column="DatePaid" type="Timestamp" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierID" />
	</class>
</hibernate-mapping>