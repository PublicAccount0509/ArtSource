		<property name="CreateDate" column="CreateDate" type="Timestamp" />
		<property name="LinkUrl" column="LinkUrl" type="String" not-null="false" />
		<property name="ActivitieOrganizers" column="ActivitieOrganizers" type="String" not-null="false" />
		<!--The foreign keys-->
		<bag name="SupplierThirdActivitiesList" cascade="none" lazy="true" table="SupplierThirdActivities">
