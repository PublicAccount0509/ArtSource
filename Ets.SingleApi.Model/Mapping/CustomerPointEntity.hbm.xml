<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.CustomerPointEntity, Ets.SingleApi.Model" table="CustomerPoint" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="PointId" column="PointId" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="Point" column="Point" type="Decimal" not-null="false" />
		<property name="PointDatetime" column="PointDatetime" type="Timestamp" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="Customer" class="Ets.SingleApi.Model.Repository.CustomerEntity, Ets.SingleApi.Model" column="CustomerId" />
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierId" />
	</class>
</hibernate-mapping>