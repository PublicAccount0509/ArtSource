<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.CuisineEntity, Ets.SingleApi.Model" table="Cuisine" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="CuisineId" column="CuisineID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="CuisineName" column="CuisineName" type="String" not-null="false" />
		<property name="CuisineName1" column="CuisineName1" type="String" not-null="false" />
		<property name="CuisineDescription" column="CuisineDescription" type="String" not-null="false" />
		<property name="CuisineImage" column="CuisineImage" type="String" not-null="false" />
		<property name="CuisineNo" column="CuisineNo" type="Int32" not-null="false" />
		<!--The foreign keys-->
		<bag name="CuisineCategoryList" cascade="none" lazy="true" table="CuisineCategory">
			<key column="CuisineID" />
			<one-to-many class="Ets.SingleApi.Model.Repository.CuisineCategoryEntity, Ets.SingleApi.Model" />
		</bag>
		<bag name="SupplierCuisineList" cascade="none" lazy="true" table="SupplierCuisine">
			<key column="CuisineID" />
			<one-to-many class="Ets.SingleApi.Model.Repository.SupplierCuisineEntity, Ets.SingleApi.Model" />
		</bag>
		<many-to-one name="SupplierMenuCategoryType" class="Ets.SingleApi.Model.Repository.SupplierMenuCategoryEntity, Ets.SingleApi.Model" column="SupplierMenuCategoryType" />
	</class>
</hibernate-mapping>