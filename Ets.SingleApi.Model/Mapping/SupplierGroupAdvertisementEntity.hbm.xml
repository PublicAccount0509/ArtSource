<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierGroupAdvertisementEntity, Ets.SingleApi.Model" table="SupplierGroupAdvertisement" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="AdvertisementId" column="AdvertisementId" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="ImagePath" column="ImagePath" type="String" />
		<property name="WIdth" column="Width" type="Int32" not-null="false" />
		<property name="Height" column="Height" type="Int32" not-null="false" />
		<property name="IsDetele" column="IsDetele" type="Boolean" />
		<property name="CreateDate" column="CreateDate" type="Timestamp" />
		<!--The foreign keys-->
		<many-to-one name="SupplierGroupLight" class="Ets.SingleApi.Model.Repository.SupplierGroupLightEntity, Ets.SingleApi.Model" column="SupplierGroupLightId" />
	</class>
</hibernate-mapping>