<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SourcePathEntity, Ets.SingleApi.Model" table="SourcePath" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="Value" column="Value" type="String" >
			<generator class="assigned"/>
		</id>
		<!--The propertys-->
		<property name="Id" column="Id" type="Int32" />
		<property name="Text" column="Text" type="String" />
		<!--The foreign keys-->
		<bag name="CustomerList" cascade="none" lazy="true" table="Customer">
			<key column="Path" />
			<one-to-many class="Ets.SingleApi.Model.Repository.CustomerEntity, Ets.SingleApi.Model" />
		</bag>
		<bag name="DeliveryList" cascade="none" lazy="true" table="Delivery">
			<key column="Path" />
			<one-to-many class="Ets.SingleApi.Model.Repository.DeliveryEntity, Ets.SingleApi.Model" />
		</bag>
	</class>
</hibernate-mapping>