<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.MarketingLogEntity, Ets.SingleApi.Model" table="MarketingLog" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="MarketingLogId" column="MarketingLogID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="CustomerId" column="CustomerID" type="Int32" not-null="false" />
		<property name="SupplierId" column="SupplierID" type="Int32" not-null="false" />
		<property name="AddDateTime" column="AddDateTime" type="Timestamp" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="MarketingMessage" class="Ets.SingleApi.Model.Repository.MarketingMessageEntity, Ets.SingleApi.Model" column="MarketingMessageID" />
	</class>
</hibernate-mapping>