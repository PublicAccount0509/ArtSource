<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.TableDeskEntity, Ets.SingleApi.Model" table="TableDesk" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="TableId" column="TableID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="TableNumber" column="TableNumber" type="Int32" />
		<property name="TableSize" column="TableSize" type="Int32" />
		<property name="Reservable" column="Reservable" type="Boolean" />
		<property name="NeedDeposit" column="NeedDeposit" type="Boolean" not-null="false" />
		<property name="IsReserved" column="IsReserved" type="Boolean" not-null="false" />
		<property name="DepositAmout" column="DepositAmout" type="Decimal" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierID" />
	</class>
</hibernate-mapping>