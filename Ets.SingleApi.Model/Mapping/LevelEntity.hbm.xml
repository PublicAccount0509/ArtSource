<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.LevelEntity, Ets.SingleApi.Model" table="Level" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="LevelId" column="LevelID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="LevelName" column="LevelName" type="String" />
		<property name="LevelDescription" column="LevelDescription" type="String" not-null="false" />
		<!--The foreign keys-->
		<bag name="LoginList" cascade="none" lazy="true" table="Login">
			<key column="LevelID" />
			<one-to-many class="Ets.SingleApi.Model.Repository.LoginEntity, Ets.SingleApi.Model" />
		</bag>
	</class>
</hibernate-mapping>