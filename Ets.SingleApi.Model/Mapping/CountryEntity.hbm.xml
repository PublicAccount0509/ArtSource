<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.CountryEntity, Ets.SingleApi.Model" table="Country" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="CountryId" column="CountryID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="CountryName" column="CountryName" type="String" />
		<!--The foreign keys-->
		<bag name="AddrProvinceList" cascade="none" lazy="true" table="Addr_Province">
			<key column="CountryID" />
			<one-to-many class="Ets.SingleApi.Model.Repository.AddrProvinceEntity, Ets.SingleApi.Model" />
		</bag>
	</class>
</hibernate-mapping>