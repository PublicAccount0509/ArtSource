<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.PaymentRecordEntity, Ets.SingleApi.Model" table="PaymentRecord" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="PaymentRecordId" column="PaymentRecordId" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="PaymentTypeId" column="PaymentTypeId" type="Int32" not-null="false" />
		<property name="OrderId" column="OrderId" type="String" not-null="false" />
		<property name="Amount" column="Amount" type="Decimal" not-null="false" />
		<property name="PaymentDate" column="PaymentDate" type="Timestamp" not-null="false" />
		<property name="PaymentMethodId" column="PaymentMethodId" type="Int32" not-null="false" />
		<property name="PayBank" column="PayBank" type="String" not-null="false" />
		<property name="OrderTypeId" column="OrderTypeId" type="Int32" not-null="false" />
	</class>
</hibernate-mapping>