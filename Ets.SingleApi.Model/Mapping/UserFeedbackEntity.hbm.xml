	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="FeedbackId" column="FeedbackId" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="UserId" column="UserId" type="Int32" not-null="false" />
		<property name="IPAddress" column="IPAddress" type="String" not-null="false" />
		<property name="Content" column="Content" type="String" not-null="false" />
		<property name="EmailOrPhone" column="EmailOrPhone" type="String" not-null="false" />
		<property name="Source" column="Source" type="String" not-null="false" />
    <property name="Path" column="Path" type="String" not-null="false" />
    <property name="CreateDate" column="CreateDate" type="Timestamp" not-null="false" />
	</class>
</hibernate-mapping>