<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.UserFeedbackEntity, Ets.SingleApi.Model" table="UserFeedback" dynamic-update="true" optimistic-lock="all" >
