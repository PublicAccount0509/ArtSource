<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.AddrProvinceEntity, Ets.SingleApi.Model" table="Addr_Province" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="ProvinceId" column="ProvinceID" type="String" >
			<generator class="assigned"/>
		</id>
		<!--The propertys-->
		<property name="ProvinceName" column="ProvinceName" type="String" />
		<property name="Hits" column="Hits" type="Int64" not-null="false" />
		<property name="Tags" column="Tags" type="String" not-null="false" />
		<!--The foreign keys-->
		<bag name="AddrCityList" cascade="none" lazy="true" table="Addr_City">
			<key column="ProvinceID" />
			<one-to-many class="Ets.SingleApi.Model.Repository.AddrCityEntity, Ets.SingleApi.Model" />
		</bag>
		<bag name="SupplierList" cascade="none" lazy="true" table="Supplier">
			<key column="AddrProvinceID" />
			<one-to-many class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" />
		</bag>
		<many-to-one name="Country" class="Ets.SingleApi.Model.Repository.CountryEntity, Ets.SingleApi.Model" column="CountryID" />
	</class>
</hibernate-mapping>