<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.OrderStatusEntity, Ets.SingleApi.Model" table="OrderStatus" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="OrderStatusId" column="OrderStatusID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="OrderStatus" column="OrderStatus" type="String" />
	</class>
</hibernate-mapping>