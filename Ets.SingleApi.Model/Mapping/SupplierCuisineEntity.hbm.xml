<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierCuisineEntity, Ets.SingleApi.Model" table="SupplierCuisine" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="SupplierCuisineId" column="SupplierCuisineID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The foreign keys-->
		<many-to-one name="Cuisine" class="Ets.SingleApi.Model.Repository.CuisineEntity, Ets.SingleApi.Model" column="CuisineID" />
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierID" />
	</class>
</hibernate-mapping>