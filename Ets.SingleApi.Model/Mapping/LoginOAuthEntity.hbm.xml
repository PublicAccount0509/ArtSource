<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.LoginOAuthEntity, Ets.SingleApi.Model" table="LoginOAuth" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="LOId" column="LOId" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="KeyName" column="KeyName" type="String" />
		<property name="SafeCode" column="safeCode" type="String" not-null="false" />
		<property name="JointLoginType" column="JointLoginType" type="Int32" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="Login" class="Ets.SingleApi.Model.Repository.LoginEntity, Ets.SingleApi.Model" column="LoginID" />
	</class>
</hibernate-mapping>