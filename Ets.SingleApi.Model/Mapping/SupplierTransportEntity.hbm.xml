<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierTransportEntity, Ets.SingleApi.Model" table="SupplierTransport" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
	<composite-id access="field">
			 <key-property name="SupplierId" column="SupplierID" type="Int32" />
 <key-property name="PublicTransportId" column="PublicTransportID" type="Int32" />

		</composite-id>
		
		<!--The foreign keys-->
		<many-to-one name="PublicTransport" class="Ets.SingleApi.Model.Repository.PublicTransportEntity, Ets.SingleApi.Model" column="PublicTransportID" />
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierID" />
	</class>
</hibernate-mapping>