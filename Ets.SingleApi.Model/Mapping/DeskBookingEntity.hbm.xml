<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.DeskBookingEntity, Ets.SingleApi.Model" table="DeskBooking" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="Id" column="Id" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="SupplierId" column="SupplierId" type="Int32" />
		<property name="OrderNo" column="OrderNo" type="Int32" />
		<property name="CreateTime" column="CreateTime" type="Timestamp" />
		<property name="Description" column="Description" type="String" not-null="false" />
		<property name="BookType" column="BookType" type="Boolean" />
		<property name="TimeType" column="TimeType" type="Int32" not-null="false" />
		<property name="NumberOfPeople" column="NumberOfPeople" type="Int32" not-null="false" />
		<property name="ReservationTime" column="ReservationTime" type="Timestamp" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="DeskType" class="Ets.SingleApi.Model.Repository.DeskTypeEntity, Ets.SingleApi.Model" column="DeskTypeId" />
		<many-to-one name="Desk" class="Ets.SingleApi.Model.Repository.SupplierDeskEntity, Ets.SingleApi.Model" column="DeskId" />
	</class>
</hibernate-mapping>