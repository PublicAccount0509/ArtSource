<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierFeatureEntity, Ets.SingleApi.Model" table="SupplierFeature" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="SupplierFeatureId" column="SupplierFeatureID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="IsEnabled" column="IsEnabled" type="Boolean" not-null="false" />
		<property name="DateUpdated" column="DateUpdated" type="Timestamp" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="Feature" class="Ets.SingleApi.Model.Repository.FeatureEntity, Ets.SingleApi.Model" column="FeatureID" />
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierID" />
	</class>
</hibernate-mapping>