	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="TimeTableDisplayId" column="TimeTableDisplayID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="OpenTime" column="OpenTime" type="Timestamp" not-null="false" />
		<property name="CloseTime" column="CloseTime" type="Timestamp" not-null="false" />
		<property name="LastDeliveryTime" column="LastDeliveryTime" type="Timestamp" not-null="false" />
	</class>
</hibernate-mapping>