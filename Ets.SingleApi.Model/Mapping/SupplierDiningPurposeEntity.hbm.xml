<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierDiningPurposeEntity, Ets.SingleApi.Model" table="SupplierDiningPurpose" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
	<composite-id access="field">
			 <key-property name="DiningPurposeId" column="DiningPurposeID" type="Int32" />
 <key-property name="SupplierId" column="SupplierID" type="Int32" />

		</composite-id>
		
		<!--The propertys-->
		<property name="VoteCount" column="VoteCount" type="Int32" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="DiningPurpose" class="Ets.SingleApi.Model.Repository.DiningPurposeEntity, Ets.SingleApi.Model" column="DiningPurposeID" />
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierID" />
	</class>
</hibernate-mapping>