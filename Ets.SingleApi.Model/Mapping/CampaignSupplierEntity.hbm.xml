<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.CampaignSupplierEntity, Ets.SingleApi.Model" table="CampaignSupplier" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="CampaignSupplierId" column="CampaignSupplierID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="CreatedOn" column="CreatedOn" type="Timestamp" not-null="false" />
		<property name="CreatedBy" column="CreatedBy" type="String" not-null="false" />
		<property name="ModifiedOn" column="ModifiedOn" type="Timestamp" not-null="false" />
		<property name="ModifiedBy" column="ModifiedBy" type="String" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="Campaign" class="Ets.SingleApi.Model.Repository.CampaignEntity, Ets.SingleApi.Model" column="CampaignID" />
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierID" />
	</class>
</hibernate-mapping>