<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.OrderDetailEntity, Ets.SingleApi.Model" table="OrderDetail" dynamic-update="true" optimistic-lock="all" >
