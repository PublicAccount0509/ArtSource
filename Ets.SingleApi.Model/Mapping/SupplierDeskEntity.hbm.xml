<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierDeskEntity, Ets.SingleApi.Model" table="SupplierDesk" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="Id" column="Id" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="DeskName" column="DeskName" type="String" not-null="false" />
		<property name="DeskNo" column="DeskNo" type="String" />
		<property name="IsDel" column="IsDel" type="Boolean" />
		<property name="CreateTime" column="CreateTime" type="Timestamp" />
		<property name="UpdateTime" column="UpdateTime" type="Timestamp" />
		<property name="SupplierId" column="SupplierId" type="Int32" />
		<property name="IsEnable" column="IsEnable" type="Boolean" />
		<property name="Description" column="Description" type="String" not-null="false" />
		<property name="ImgPath" column="ImgPath" type="String" not-null="false" />
		<!--The foreign keys-->
		<bag name="DeskBookingList" cascade="none" lazy="true" table="DeskBooking">
			<key column="DeskId" />
			<one-to-many class="Ets.SingleApi.Model.Repository.DeskBookingEntity, Ets.SingleApi.Model" />
		</bag>
		<many-to-one name="DeskType" class="Ets.SingleApi.Model.Repository.DeskTypeEntity, Ets.SingleApi.Model" column="DeskTypeId" />
	</class>
</hibernate-mapping>