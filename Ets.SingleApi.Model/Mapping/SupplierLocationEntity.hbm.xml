<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierLocationEntity, Ets.SingleApi.Model" table="SupplierLocation" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="SupplierLocationId" column="SupplierLocationID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The foreign keys-->
		<many-to-one name="Location" class="Ets.SingleApi.Model.Repository.LocationEntity, Ets.SingleApi.Model" column="LocationID" />
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierID" />
	</class>
</hibernate-mapping>