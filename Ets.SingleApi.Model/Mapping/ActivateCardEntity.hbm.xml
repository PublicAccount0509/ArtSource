<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.ActivateCardEntity, Ets.SingleApi.Model" table="ActivateCard" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="Id" column="ID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="CardNumber" column="CardNumber" type="String" />
		<property name="Name" column="Name" type="String" />
		<property name="Sex" column="Sex" type="Int32" />
		<property name="Phone" column="Phone" type="String" />
		<property name="Email" column="Email" type="String" />
		<property name="ActivateTiem" column="ActivateTiem" type="Timestamp" />
		<property name="PhoneMAC" column="PhoneMAC" type="String" not-null="false" />
	</class>
</hibernate-mapping>