<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.CuisineCategoryEntity, Ets.SingleApi.Model" table="CuisineCategory" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="CuisineCategoryId" column="CuisineCategoryID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The foreign keys-->
		<many-to-one name="Category" class="Ets.SingleApi.Model.Repository.CategoryEntity, Ets.SingleApi.Model" column="CategoryID" />
		<many-to-one name="Cuisine" class="Ets.SingleApi.Model.Repository.CuisineEntity, Ets.SingleApi.Model" column="CuisineID" />
	</class>
</hibernate-mapping>