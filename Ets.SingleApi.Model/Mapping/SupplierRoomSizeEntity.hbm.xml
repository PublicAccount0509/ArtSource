<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierRoomSizeEntity, Ets.SingleApi.Model" table="SupplierRoomSize" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="RoomSizeId" column="RoomSizeId" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="RoomName" column="RoomName" type="String" />
		<property name="MinConsumption" column="MinConsumption" type="Int32" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="Supplier" class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" column="SupplierId" />
	</class>
</hibernate-mapping>