<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.ActivityEntity, Ets.SingleApi.Model" table="Activity" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="Id" column="ID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="Name" column="Name" type="String" not-null="false" />
		<property name="ChineseName" column="ChineseName" type="String" not-null="false" />
		<property name="Comments" column="Comments" type="String" not-null="false" />
	</class>
</hibernate-mapping>