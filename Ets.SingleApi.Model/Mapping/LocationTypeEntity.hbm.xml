<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.LocationTypeEntity, Ets.SingleApi.Model" table="LocationType" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="LocationTypeId" column="LocationTypeID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="LocationType" column="LocationType" type="String" />
		<!--The foreign keys-->
		<bag name="LocationList" cascade="none" lazy="true" table="Location">
			<key column="LocationTypeID" />
			<one-to-many class="Ets.SingleApi.Model.Repository.LocationEntity, Ets.SingleApi.Model" />
		</bag>
	</class>
</hibernate-mapping>