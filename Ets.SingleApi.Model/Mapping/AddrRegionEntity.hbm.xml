<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.AddrRegionEntity, Ets.SingleApi.Model" table="Addr_Region" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="RegionId" column="RegionID" type="String" >
			<generator class="assigned"/>
		</id>
		<!--The propertys-->
		<property name="RegionName" column="RegionName" type="String" />
		<property name="Hits" column="Hits" type="Int64" not-null="false" />
		<property name="Tags" column="Tags" type="String" not-null="false" />
		<property name="RegionNo" column="RegionNo" type="Int32" />
		<!--The foreign keys-->
		<bag name="AddrBusinessAreaList" cascade="none" lazy="true" table="Addr_BusinessArea">
			<key column="RegionID" />
			<one-to-many class="Ets.SingleApi.Model.Repository.AddrBusinessAreaEntity, Ets.SingleApi.Model" />
		</bag>
		<bag name="SupplierList" cascade="none" lazy="true" table="Supplier">
			<key column="AddrRegionID" />
			<one-to-many class="Ets.SingleApi.Model.Repository.SupplierEntity, Ets.SingleApi.Model" />
		</bag>
		<many-to-one name="City" class="Ets.SingleApi.Model.Repository.AddrCityEntity, Ets.SingleApi.Model" column="CityID" />
	</class>
</hibernate-mapping>