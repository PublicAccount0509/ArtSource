<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Ets.SingleApi.Model.Repository.SupplierCategoryEntity, Ets.SingleApi.Model" table="SupplierCategory" dynamic-update="true" optimistic-lock="all" >
	 <!--The cache-->
	 <cache usage="read-write"/>
	 <!--The primary keys-->
		<id name="SupplierCategoryId" column="SupplierCategoryID" type="Int32" >
			<generator class="native"/>
		</id>
		<!--The propertys-->
		<property name="SupplierId" column="SupplierID" type="Int32" />
		<property name="CategoryOrder" column="CategoryOrder" type="Int32" />
		<property name="SupplierMenuCategoryId" column="SupplierMenuCategoryId" type="Int32" not-null="false" />
		<!--The foreign keys-->
		<many-to-one name="Category" class="Ets.SingleApi.Model.Repository.CategoryEntity, Ets.SingleApi.Model" column="CategoryID" />
	</class>
</hibernate-mapping>