<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Ets.SingleApi.Model.Repository.LogisticsAreaEntity, Ets.SingleApi.Model" table="LogisticsArea" dynamic-update="true" optimistic-lock="all" >
    <!--The cache-->
    <cache usage="read-write"/>
    <!--The primary keys-->
    <id name="AreaId" column="AreaId" type="Int32" >
      <generator class="native"/>
    </id>
    <!--The propertys-->
    <property name="AreaName" column="AreaName" type="String" />
    <property name="Map" column="Map" type="BinaryBlob" not-null="false" />
    <property name="RegionCode" column="RegionCode" type="String" not-null="false" />
    <property name="Note" column="Note" type="String" not-null="false" />
    <property name="Coods" column="Coods" type="String" not-null="false" />
    <property name="LogisticsId" column="LogisticsId" type="Int32" not-null="false" />
    <property name="IsEnabled" column="IsEnabled" type="Boolean" not-null="false" />
  </class>
  <sql-query name="Pro_LogisticsArea">
    <return-scalar column="AreaId" type="Int32" />
    exec Pro_LogisticsArea :Lng,:Lat
  </sql-query>
</hibernate-mapping>