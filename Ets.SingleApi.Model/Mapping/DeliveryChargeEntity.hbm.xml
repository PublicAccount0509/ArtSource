<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Ets.SingleApi.Model.Repository.DeliveryChargeEntity, Ets.SingleApi.Model" table="DeliveryCharge" dynamic-update="true" optimistic-lock="all" >
    <!--The cache-->
    <cache usage="read-write"/>
    <!--The primary keys-->
    <id name="DCChargeId" column="DCChargeID" type="Int32" >
      <generator class="native"/>
    </id>
    <!--The propertys-->
    <property name="SupplierId" column="SupplierID" type="Int32" not-null="false" />
    <property name="Miles" column="Miles" type="Int32" not-null="false" />
    <property name="DeliveryCharge" column="DeliveryCharge" type="Decimal" not-null="false" />
  </class>
</hibernate-mapping>