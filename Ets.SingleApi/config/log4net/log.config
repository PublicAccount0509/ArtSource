<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <log4net>
    <!-- OFF, FATAL, ERROR, WARN, INFO, DEBUG, ALL -->
    <!-- Set root logger level to ERROR and its appenders -->
    <!--<root>
      <level value="ALL"/>
      <appender-ref ref="SysAppender"/>
    </root>-->
    <logger name="Ets.SingleApi.WapControllers">
      <level value="DEBUG"/>
      <appender-ref ref="WapControllersAppender" />
    </logger>

    <logger name="Ets.SingleApi.Controllers">
      <level value="DEBUG"/>
      <appender-ref ref="ControllersAppender" />
    </logger>

    <logger name="Ets.SingleApi.Services">
      <level value="DEBUG"/>
      <appender-ref ref="ServicesAppender" />
    </logger>

    <logger name="Ets.SingleApi.DetailServices">
      <level value="DEBUG"/>
      <appender-ref ref="DetailServicesAppender" />
    </logger>

    <logger name="Ets.SingleApi.ExternalServices">
      <level value="DEBUG"/>
      <appender-ref ref="ExternalServicesAppender" />
    </logger>

    <logger name="Ets.SingleApi.Filter">
