    </logger>

    <logger name="Ets.SingleApi.Debug">
      <level value="DEBUG"/>
      <appender-ref ref="DebugMessageAppender" />
    </logger>

    <logger name="NHibernate">
      <level value="OFF"/>
      <appender-ref ref="NHibernateAppender" />
    </logger>

    <!--The format recording the log of IBB-->
    <appender name="WapControllersAppender" type="log4net.Appender.RollingFileAppender,log4net" >
      <param name="File" value="Logs/WapControllers/" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyy-MM-dd&quot;.txt&quot;" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n" />
        <param name="Header" value="&#13;&#10;----------------------header--------------------------&#13;&#10;" />
        <param name="Footer" value="&#13;&#10;----------------------footer--------------------------&#13;&#10;" />
      </layout>
    </appender>

